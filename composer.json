{
    "name": "webjetclouds-agency/wjc-opensource-php",
    "type": "project",
    "description": "Website simple site without Database SQL",
    "keywords": ["php", "simple", "without sql"],
    "homepage": "https://github.com/webjetclouds-agency/wjc-opensource-php",
    "license": "GPL-2.0-or-later",
    "config": {
      "optimize-autoloader": true,
      "classmap-authoritative": true,
      "apcu-autoloader": true,
      "platform": {
        "php": "8.4"
      },
      "vendor-dir": "vendor"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "support": {
      "issues": "https://support.webjet.cloud/",
      "docs": "https://git.webjet.cloud/"
    },
    "require": {
      "php": "8.4",        
      "icamys/php-sitemap-generator": "^5.0",
      "gargron/fileupload": "^1.4",
      "matthiasmullie/minify": "^1.3",
      "google/recaptcha": "^1.3",
      "phpmailer/phpmailer": "^6.9",
      "adodb/adodb-php": "^5.22",
      "giggsey/libphonenumber-for-php": "^9.0",
      "ext-json": "*",
      "ext-simplexml": "*",
      "dejurin/php-google-translate-for-free": "^1.0",
      "monolog/monolog": "^2.0",
      "spatie/schema-org": "^3.23",
      "cocur/slugify": "^4.6",
      "melbahja/seo": "^2.1",
      "symfony/cache": "^7.2"
    },
    "minimum-stability": "stable"
  }
